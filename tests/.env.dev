# 动态数据库引擎配置示例
# 多数据库配置支持

# MySQL
DB_ENGINE_DATABASES__CREDIBLE__NAME=credible
DB_ENGINE_DATABASES__CREDIBLE__URL=mysql+pymysql://user:password@localhost:1234/mysql_db

# 交易中心 oceanbase oracle 模式
DB_ENGINE_OCEANBASES__TRADE__JDBC=jdbc:oceanbase://localhost:2883/OCEANBASE_DB?useUnicode=true&characterEncoding=UTF-8&rewriteBatchedStatements=true
DB_ENGINE_OCEANBASES__TRADE__USERNAME=user
DB_ENGINE_OCEANBASES__TRADE__PASSWORD=password
DB_ENGINE_OCEANBASES__TRADE__DRIVER=com.oceanbase.jdbc.Driver
DB_ENGINE_OCEANBASES__TRADE__CLIENT_JAR=/soft/oceanbase/oceanbase-client-2.4.9.jar

# 配置工作路径
DB_ENGINE_WORK_PATH=tests\temp
DB_ENGINE_LOG_PATH=tests\temp
DB_ENGINE_FILE_PATH=tests\temp
DB_ENGINE_DEBUG=false
